<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">

    <f:entry title="${%RetentionTimeInHours}" field="retentionInHours">
        <f:textbox default="24"/>
    </f:entry>

    <f:entry title="${%Pool_Size}" field="poolSize">
        <f:textbox default="3" />
    </f:entry>

    <f:entry title="${%Spare_Pool}" field="sparePool">
        <f:checkbox/>
    </f:entry>

    <f:entry title="${%Single_Use}" field="singleUse">
        <f:checkbox/>
    </f:entry>

</j:jelly>
